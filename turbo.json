{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "dependsOn": [],
      "cache": false
    },
    "prepare": {
      "inputs": ["src/**/*.tsx", "src/**/*.ts"],
      "outputs": ["dist"]
    },
    "lint": {
      "dependsOn": [],
      "inputs": [
        "src/**/*.tsx",
        "src/**/*.ts",
        "test/**/*.ts",
        "test/**/*.tsx",
        "__test__/**/*.ts",
        "__test__/**/*.tsx"
      ],
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts"],
      "outputs": ["dist", ".next"]
    },
    "test": {
      "dependsOn": ["build"],
      "inputs": [
        "src/**/*.tsx",
        "src/**/*.ts",
        "test/**/*.ts",
        "test/**/*.tsx",
        "__test__/**/*.ts",
        "__test__/**/*.tsx"
      ],
      "outputs": []
    },
    "test:e2e": {
      "dependsOn": ["build"],
      "inputs": [
        "src/**/*.tsx",
        "src/**/*.ts",
        "test/**/*.ts",
        "test/**/*.tsx",
        "__test__/**/*.ts",
        "__test__/**/*.tsx"
      ],
      "outputs": []
    }
  }
}
